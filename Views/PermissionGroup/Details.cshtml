@model GDR.Models.PermissionGroup

@{
    ViewData["Title"] = "Detalhes do grupo de permissão " + @Model.GroupName;
}

    <h2>Detalhes do grupo de permissão @Model.GroupName</h2>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.GroupName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.GroupName) &nbsp;
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Domain)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Domain) &nbsp;
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AccessType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AccessType)
        </dd>
    </dl>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User) &nbsp;
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ChangeDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ChangeDate) &nbsp;
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Index" class="btn btn-success">
        <i class="fa fa-backward"></i> Voltar para lista
    </a>

    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-success">
        <i class="fa fa-edit"></i> Editar
    </a>
</div>